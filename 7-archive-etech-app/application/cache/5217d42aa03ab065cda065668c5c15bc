		function add_Schedule(userid)
		{
			hide_error_messages();
			var Location=$("#Location").val();
			var User=userid;
			var Color=$("#Color").val();
			var Schedule_Date=$("#Schedule_Date").val();
			var flag=1;
			
			if(Location=='')
			{
				$("#Location").closest('.form-group').addClass('has-error');
				$("#Location_error").show().html("<strong>location Required</strong>");
				flag=0;
			}
			
			
			if(User=='')
			{
				$("#User").closest('.form-group').addClass('has-error');
				$("#User_error").show().html("<strong>User Required</strong>");
				flag=0;
			}
			
			if(Schedule_Date=='')
			{
				$("#Schedule_Date").closest('.form-group').addClass('has-error');
				$("#Schedule_Date_error").show().html("<strong>Schedule Date Required</strong>");
				flag=0;
			}
			
			if(flag==0)
			{
				return false;
			}
			else
			{
						$.ajax({ 
					
								type: "POST", 
								url: base_url+'index.php/Client/adduser_Schedule',
								data:$("#add_from").serialize(),
								success: function(resp) 
								{
									if(resp==1)
									{
									  //alert('Added Successfully'); 
									  location.reload();
									}
								}
						 });
			}
			
		}
		
		function delete_userSchedule()
		{
			
			$("#delete_model").modal('show');
			var id=$("#event_id").val();
			$("#scheduleid").val(id);
			return false;
			
		/*	var status=confirm("Are you sure want to delete schedule");
			if(status)
			{
							 $.ajax({ 
						
