	$(".fc-next-button").click(function()
	{
		var n=[];
		var current_month=parseInt($("#next_month").val());
		n.push(current_month);	
		 var date = new Date();
		
		
		var today = new Date();
		var dd = today.getDate();
		var mm = today.getMonth()+2; //January is 0!
		var mm1 = today.getMonth(); //January is 0!
		
		var yyyy = today.getFullYear();
		if(dd<10){
			dd='0'+dd;
		} 
		if(mm<10){
			mm='0'+mm;
		} 
		var nextday = yyyy+'-'+mm+'-'+dd;
		//alert(global_defaults);
		//return false;

		//calendar_init(date,displaytype,nextday);
		get_calendarderevents(start,end,mm,nextday,n);
		
		
	});
	
