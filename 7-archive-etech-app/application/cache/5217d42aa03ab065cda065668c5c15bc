	function get_calendarderevents(start,end,month,mdata,n)
	{
		$("#calendar_loader").show();
		var date = new Date();
		var user_id=1;
		var today = new Date();
		var dd = today.getDate();
		var mm = today.getMonth()+2; 
		var yyyy = today.getFullYear();
		if(dd<10){
			dd='0'+dd;
		} 
		if(mm<10){
			mm='0'+mm;
		} 
		var nextday = yyyy+'-'+mm+'-'+dd;

								$.ajax({  
								  url:base_url+'index.php/Task_controller/get_events_info',
								  type:'POST',
								  dataType: 'json',
								  data: {
									  date:date,
									  user_id:user_id,
									  start:start,
									  end:end,
									  months:month
									  },
								  success: function(resp)
									{
										$("#calendar_loader").hide();
										//global_defaults=global_defaults.concat(resp);
										$('#calendar').fullCalendar('destroy');
										defaultEvents=resp;
									
										calendar_init(date,displaytype,mdata);
									
										$(".fc-next-button").click(function()
											{
												
												 var date = new Date();
												var today = new Date();
												var dd = today.getDate();
												var mm = parseInt(month)+1; 

												var yyyy = today.getFullYear();
												if(dd<10){
													dd='0'+dd;
												} 
												if(mm<10){
													mm='0'+mm;
												}
												if(mm>12)
												{
													mm='01';
													yyyy = today.getFullYear()+1;
												}
												if(mm<01)
												{
													mm='12';
													yyyy = today.getFullYear();
												}
												var nextday = yyyy+'-'+mm+'-'+dd;
												
												get_calendarderevents(start,end,mm,nextday,n);
	
											});
											
											$(".fc-prev-button").click(function()
											{
												 var date = new Date();
												var today = new Date();
												var dd = today.getDate();
												var mm1 = parseInt(month)-1; 
												
												var yyyy = today.getFullYear();
												if(dd<10){
													dd='0'+dd;
												} 
												if(mm1<10){
													mm1='0'+mm1;
												}
												
												if(mm1>12)
												{
													mm1='01';
													yyyy = today.getFullYear()+1;
												}
												if(mm1<01)
												{
													mm1='12';
													yyyy = today.getFullYear();
												} 
												
												var lastday = yyyy+'-'+mm1+'-'+dd;
												get_calendarderevents(start,end,mm1,lastday,n);

											});
									}
								});
		}
		
