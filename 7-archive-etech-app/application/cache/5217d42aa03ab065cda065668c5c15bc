    },
    eventClick: function(calEvent, jsEvent, view) {
		
		jQuery('#edit_event').modal('show');
			$("#eLocations").val(calEvent.location_id);
			 $('#eLocations').multiselect('select',calEvent.location_id); 
			$("#eUsers").val(calEvent.user_id);
			$("#eColor").val(calEvent.color);
			$("#estart_time").val(calEvent.schedule_time);
			$("#eend_time").val(calEvent.end_time);
			$("#event_id").val(calEvent.schedule_id);
			$("#eSchedule_Date").val(calEvent.schedule_date);
			
		
     	 console.log(event);
    },
    eventDrop: function (event, delta, revertFunc) {
		//alert(Object.values(event));
		//alert(event.id);
		/*for (var key in event){
    alert(key, event[key]);
    }*/
		if(event.id=="doc"){
		
			revertFunc();
			return false;
		}
		
	
		
      var from_hour=(moment(event._start).format('HH')<10 ? moment(event._start).format('HH').substring(1):moment(event._start).format('HH'));
	  var from_min=(moment(event._start).format('mm')<10 ? moment(event._start).format('mm').substring(1):moment(event._start).format('mm'));
	  
	  var to_hour=(moment(event._end).format('HH')<10 ? moment(event._end).format('HH').substring(1):moment(event._end).format('HH'));
	  var to_min=(moment(event._end).format('mm')<10 ? moment(event._end).format('mm').substring(1):moment(event._end).format('mm'));
	  
	  var docid=event.resources;
	  var schedule_id=event.id;
	  
	  //alert(event.resources);
	  
	var schedule_date=moment(event._start).format('YYYY-MM-DD');
	//alert(schedule_date);
	 
	  var selectionStart = moment(event.start); 
            var d = new Date();
			var month = d.getMonth()+1;
			var day = d.getDate();
			var output = d.getFullYear() + '/' +
				(month<10 ? '0' : '') + month + '/' +
				(day<10 ? '0' : '') + day;
				
		    var today = moment(output);
			
	
      console.log(event);
    },
	eventResize:function( event, delta, revertFunc, jsEvent, ui, view ) {
		
		if(event.id=="doc"){
		
			revertFunc();
			return false;
		}
		
				var from_hour=(moment(event._start).format('HH')<10 ? moment(event._start).format('HH').substring(1):moment(event._start).format('HH'));
	  var from_min=(moment(event._start).format('mm')<10 ? moment(event._start).format('mm').substring(1):moment(event._start).format('mm'));
	  
	  var to_hour=(moment(event._end).format('HH')<10 ? moment(event._end).format('HH').substring(1):moment(event._end).format('HH'));
	  var to_min=(moment(event._end).format('mm')<10 ? moment(event._end).format('mm').substring(1):moment(event._end).format('mm'));
	  
	  var docid=event.resources;
	  var schedule_id=event.id;
	  
	  //alert(event.resources);
	  
	var schedule_date=moment(event._start).format('YYYY-MM-DD');
	 
	  var selectionStart = moment(event.start); 
            var d = new Date();
			var month = d.getMonth()+1;
			var day = d.getDate();
			var output = d.getFullYear() + '/' +
				(month<10 ? '0' : '') + month + '/' +
				(day<10 ? '0' : '') + day;
				
		    var today = moment(output);
			
