			}*/
		}
		
		
		
		function delete_cancel()
		{
			$("#delete_model").modal('hide');
		}
		
// for delete schedule 

function delete_schedule(id)
{
	var status=confirm("Are you sure want to delete schedule");
	if(status)
	{
		             $.ajax({ 
				
							type: "POST", 
							url: base_url+'index.php/Client/delete_schedule',
							data:
							{
								id:id
							},
							success: function(resp) 
							{
								if(resp==1)
								{
								  //alert('Deleted Successfully'); 
								  location.reload();
								}
							}
					 });
	}
}
	
function bulk_Schedule()
		{
			var file=$("#file").val();
			 if(file!=0)
						{                                               
										var extension = file.split('.').pop().toUpperCase();
													  if (extension!="CSV" && extension!="csv")
													  {
														 $("#image-error").html("Enter Valid Format");
														 return false;
													  }
													  else
													  {
															$("#image-error").html("");
													  }
						}
			
		}	

	function delete_bulkstatus()
	{
		var status=[];
		$(".status").each( function() { 
			 if($(this).prop('checked'))
			 {
				 status.push($(this).val());
			 }
		
		});
		
		if(status.length<1)
		{
			alert('Please select check boxes');
			return false;
		}
				   $.ajax({
									type:'POST',
									url:base_url+'index.php/Client/delete_bulk_schedules',                       
									 data:
									 {  
										status:status,
									 },                        
									 success:function(resp)
									{
										if(resp==1)
										{
											///alert("Deleted Successfully");
										    window.location.reload();
										}
										
									}
					  });	
					  
					  
			 
		
	}	
	
	
	function change_schedule(status)
	{
		var schedule_id=$("#dragschedule_id").val();
		var dragdate=$("#dragstart").val();
		
		 $.ajax({
			 
			 type:'POST',
			 url:base_url+'index.php/Client/add_eventss',                       
			 data:
			 {  
					schedule_id:schedule_id,
					status:status,
					start:dragdate
			},                        
			success:function(resp)
			{
				//alert(resp);
				if(resp==1)
				{
					///alert("Deleted Successfully");
					 location.reload();
				}
										
			}
									
			});	
	
	}
	
