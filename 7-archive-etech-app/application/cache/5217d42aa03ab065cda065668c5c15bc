		// for hide add from hide_error_messages
		function hide_error_messages()
		{
				 $(".alert").each(function(){
					$(this).hide(); 
				 });
				 $(".form-group").each(function(){
					$(this).removeClass('has-error'); 
				 });
		}	
			
		function update_Schedule()
		{
			
			hide_error_messages();
			var Location=$("#eLocations").val();
			var User=$("#eUsers").val();
			var Schedule_Date=$("#eSchedule_Date").val();
			var flag=1;
			
			if(Location=='')
			{
				$("#Location1").closest('.form-group').addClass('has-error');
				$("#Location1_error").show().html("<strong>location Required</strong>");
				flag=0;
			}
			
			
			if(User=='')
			{
				$("#User1").closest('.form-group').addClass('has-error');
				$("#User1_error").show().html("<strong>User Required</strong>");
				flag=0;
			}
			
			if(Schedule_Date=='')
			{
				$("#Schedule_Date1").closest('.form-group').addClass('has-error');
				$("#Schedule_Date1_error").show().html("<strong>Schedule Date Required</strong>");
				flag=0;
			}
			
			if(flag==0)
			{
				return false;
			}
			else
			{
						$.ajax({ 
					
								type: "POST", 
								url: base_url+'index.php/Client/update_Schedule',
								data:$("#edit_from").serialize(),
								success: function(resp) 
								{
									if(resp==1)
									{
									  
									  location.reload();
									}
								}
						 });
			}
			
		}
		
		function edit_schedule(id)
		{
			     hide_error_messages();
				 $.ajax({ 
							type: "POST", 
							url: base_url+'index.php/Client/get_scheduledata',
							data:
							{
								id:id
							},
							success: function(resp) 
							{
								var jdata=jQuery.parseJSON(resp);
								$("#Location1").val(jdata[0]['location_id']);
								$("#User1").val(jdata[0]['user_id']); 
								$("#Schedule_Date1").val(jdata[0]['schedule_date']+' '+jdata[0]['schedule_time']);
								$("#Scheduleid").val(jdata[0]['schedule_id']);
							}
					 });
				 
		}	
		
				
		function adding_Schedule(userid)
		{
			//alert(defaultEvents);
			hide_error_messages();
			var Location=$("#Locations").val();
			var User=userid;
			var Color=$("#Color1").val();
			var Schedule_Date=$("#Schedule_Dates").val();
			var Schedule_Time=$("#Schedule_Time").val();
			var flag=1;
			
			if(Location=='')
			{
				$("#Locations").closest('.form-group').addClass('has-error');
				$("#Locations_error").show().html("<strong>location Required</strong>");
				flag=0;
			}
			
			
			if(User=='')
			{
				$("#Users").closest('.form-group').addClass('has-error');
				$("#Users_error").show().html("<strong>User Required</strong>");
				flag=0;
			}
			if(Schedule_Time=='')
			{
				$("#Schedule_Time").closest('.form-group').addClass('has-error');
				$("#Schedule_Time_error").show().html("<strong>User Required</strong>");
				flag=0;
			}
			
			
			if(flag==0)
			{
				return false;
			}
			else
			{
				var date = new Date();
				var displaytype='resourceDay';
				var next='next';
				var mdata=new Date();
				
				
				
						$.ajax({ 
					
								type: "POST", 
								url: base_url+'index.php/Client/addinguser_Schedule',
								data:$("#add_froms").serialize(),
								success: function(resp) 
								{
									//alert(resp);
									//return false;
									//defaultEvents=[];
									//$("#event-modal").hide();
									//defaultEvents.push(resp);
									if(resp==1)
									{
									  alert('Added Successfully'); 
									  location.reload();
									}
									
							/*		$('#calendar').fullCalendar('destroy');
										defaultEvents=resp;
									
										calendar_init(date,displaytype,mdata);
									
										$(".fc-next-button").click(function()
											{
												
												 var date = new Date();
												var today = new Date();
												var dd = today.getDate();
												 var month = today.getMonth();
												var mm = parseInt(month)+1; 

												var yyyy = today.getFullYear();
												if(dd<10){
													dd='0'+dd;
												} 
												if(mm<10){
													mm='0'+mm;
												}
												if(mm>12)
												{
													mm='01';
													yyyy = today.getFullYear()+1;
												}
												if(mm<01)
												{
													mm='12';
													yyyy = today.getFullYear();
												}
												var nextday = yyyy+'-'+mm+'-'+dd;
												
												get_calendarderevents(start,end,mm,nextday,n);
	
											});
											
