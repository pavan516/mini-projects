		//////////////////////////////calendar init/////////////////////////////////////////////
		
function calendar_init(da,displaytype,mdate)
{
	//alert(mdate);
	//alert(minimumtime+'--'+maximumtime)
	var dt = new Date();
    var curtime = dt.getHours() + ":" + dt.getMinutes() + ":" + dt.getSeconds();
	 
	//alert(da);
  $('#calendar').fullCalendar({
    header: {
      left: 'prev,next today',
     center: 'title',
      right: 'month,agendaWeek,agendaDay'
    },
	defaultDate: mdate,
	defaultView: 'month',
	events:defaultEvents,
    editable: true,
    droppable: true,
	draggable:true,
	eventLimit: 4, 
	nowIndicator:true,
	slotDuration:"00:15:00",
	allDayText: 'Day',
	allDaySlot:false,
	scrollTime: curtime,
	minTime: '08:00:00',
    maxTime: '19:00:00', 
	handleWindowResize: true,   
    height: $(window).height() - 200,
    /*resources: [{
      'id': 'resource1',
      'name': 'Resource 1'
    }, {
      'id': 'resource2',
      'name': 'Resource 2'
    }, {
      'id': 'resource3',
      'name': 'Resource 3',
      'className': ['red']
    }, {
      'id': 'resource4',
      'name': 'Resource 4',
      'className': ['red']
    }, {
      'id': 'resource5',
      'name': 'Resource 5',
      'className': ['red']
    }],*/
	//resources: resourceslist,
  

  /*events: [
				{ id: '1', resourceId: '1', start: '2016-10-06', end: '2016-09-08', title: 'event 1' },
				{ id: '2', resourceId: '2', start: '2016-10-07T09:00:00', end: '2016-10-07T14:00:00', title: 'event 2' },
				
			], */
    // the 'ev' parameter is the mouse event rather than the resource 'event'
    // the ev.data is the resource column clicked upon
    selectable: true,
    selectHelper: true,
    select: function(start, end, ev) {
      console.log(start);
      console.log(end);
      console.log(ev.data); // resources
	 // alert(Object.values(ev.data));
	// alert(ev.data['id']);
	//alert(ev.data)
	 resourceid='';
	 if (typeof ev.data === 'undefined' || ev.data==null){
		 var resourceid='';
	 }else{
	      var resourceid=ev.data['id'];
	 }
	 
	  var from_hour=(moment(start).format('HH')<10 ? moment(start).format('HH').substring(1):moment(start).format('HH'));
	  var from_min=(moment(start).format('mm')<10 ? moment(start).format('mm').substring(1):moment(start).format('mm'));
	  
	  var to_hour=(moment(end).format('HH')<10 ? moment(end).format('HH').substring(1):moment(end).format('HH'));
	  var to_min=(moment(end).format('mm')<10 ? moment(end).format('mm').substring(1):moment(end).format('mm'));
	
	  		var selectionStart = moment(start); 
            var d = new Date();
			var month = d.getMonth()+1;
			var day = d.getDate();
			var output = d.getFullYear() + '/' +
				(month<10 ? '0' : '') + month + '/' +
				(day<10 ? '0' : '') + day;
				
		    var today = moment(output);
			/////////////////////////////////////////////////////////////////////////////////////////////
			// var $this = this;
				 var dateFormat = $.datepicker.formatDate('yy-mm-dd', new Date(start));
			
			
			 $("#aSchedule_Date").val(dateFormat);
			$("#Schedule_Dates").val(dateFormat);
		
			jQuery('#event-modal').modal('show');
			
			////////////////////////////////////////////////////////////////////////////////////////////////
			
			/*if (selectionStart >= today) {
	  		
				$.ajax({  
				  url:baseurl+'index.php/Home/schedule_form',
				  type:'POST',
				  data: {
					  		location:$("#locs").val(),
							resourceid:resourceid,
							d:moment(start).format('YYYY-MM-DD'),
						},
				  success: function(resp)
					{
						
						$("#scheduleleevent").modal('show');
						$("#schedule_resp").html(resp);
						
						client_autoinit();
						$("#schedule_date").val(moment(start).format('YYYY-MM-DD'));
						$(".schedulebutton").attr('onclick','add_schedule()');
